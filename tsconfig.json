{
  "extends": "@quasar/app-vite/tsconfig-preset",
  "compilerOptions": {
    "baseUrl": ".",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "composite": true,
    "module": "esnext",
    "moduleResolution": "node",
    "paths": {
      "src/*": ["src/*"],
      "app/*": ["*"],
      "components/*": ["src/components/*"],
      "data/*": ["src/data/*"],
      "layouts/*": ["src/layouts/*"],
      "models/*": ["src/models/*"],
      "pages/*": ["src/pages/*"],
      "assets/*": ["src/assets/*"],
      "boot/*": ["src/boot/*"],
      "stores/*": ["src/stores/*"]
    }
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "src/data/**/*.json",
    "./package.json"
  ]
  // "references": [
  //   {
  //     "path": "./tsconfig.node.json"
  //   }
  // ]
}
